{
  "name": "some-ui-mdx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rollup --bundleConfigAsCjs -c --no-cache && pnpm build:tsc-alias",
    "build:tsc-alias": "tsc-alias -p tsconfig.json",
    "clean": "rimraf ./dist && rimraf .turbo && rimraf node_modules && rimraf rimraf tsconfig.tsbuildinfo && rimraf .eslintcache",
    "lint": "pnpm --package=turbo@latest -- turbo lint",
    "lint:fix": "eslint --fix",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --cache",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "lucide-react": "0.436.0",
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "next-contentlayer2": "^0.4.6",
    "some-ui-utils": "workspace:*",
    "some-ui-shared": "workspace:*"
  },
  "dependencies": {
    "some-ui-shared": "workspace:*",
    "some-ui-utils": "workspace:*",
    "zod": "3.23.8",
    "unist-builder": "3.0.0",
    "lucide-react": "0.436.0",
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "next-contentlayer2": "^0.4.6"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@some-ui/tsconfig": "workspace:*",
    "@some-ui/rollup-config": "workspace:*"
  }
}
