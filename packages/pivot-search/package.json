{
  "name": "@some-ui/pivot-search",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist && rmraf ./node_modules",
    "dev": "vite",
    "build": "rollup --bundleConfigAsCjs -c",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --cache",
    "lint:md": "eslint \"**/*.{md,mdx}\" --cache",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "classnames": "^2.2.6",
    "@some-ui/shared": "workspace:*"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@some-ui/tsconfig": "workspace:*"
  }
}
