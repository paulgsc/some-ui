{
  "name": "some-ui-utils",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rollup --bundleConfigAsCjs -c --no-cache && pnpm build:tsc-alias",
    "build:tsc-alias": "tsc-alias -p tsconfig.json",
    "clean": "rimraf ./dist && rimraf .turbo && rimraf node_modules && rimraf rimraf tsconfig.tsbuildinfo && rimraf .eslintcache",
    "lint": "pnpm --package=turbo@latest -- turbo lint:js typecheck prettier",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --cache",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache",
    "typecheck": "tsc --noEmit",
    "postinstall": "pnpm build"
  },
  "dependencies": {
    "tailwind-merge": "^2.4.0",
    "clsx": "^2.1.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@some-ui/tsconfig": "workspace:*",
    "@some-ui/rollup-config": "workspace:*"
  }
}
